{
    "name": "milito/todolist",
    "description": "todo list react php",
    "type": "project",
    "authors": [
        {
            "name": "Milad Purahmad",
            "email": "milad.purahmad78@gmail.com"
        }
    ],
    "minimum-stability": "stable",
    "require": {
        "php": "^8.0.2",
        "react/http": "^1.6",
        "vlucas/phpdotenv": "^5.4",
        "react/mysql": "^0.5.6",
        "league/container": "^4.2",
        "nikic/fast-route": "^1.3",
        "respect/validation": "^2.2",
        "symfony/console": "^6.0.8",
        "monolog/monolog": "^2.5",
        "symfony/finder": "~6.0.8",
        "ext-pdo": "*",
        "doctrine/migrations": "^3.5",
        "wyrihaximus/react-child-process-promise-closure": "^1.1",
        "illuminate/config": "^9.13",
        "narrowspark/mimetypes": "^1.6"
    },
    "autoload": {
        "psr-4": {
            "Core\\" : "core",
            "App\\" : "src",
            "React\\Filesystem\\": "Filesystem"
        },
        "files": [
            "core/Helpers/helpers.php"
        ]
    },
    "config": {
        "allow-plugins": {
            "kylekatarnls/update-helper": true
        }
    }
}
